{
  "name": "mobx-stored",
  "version": "0.10.0",
  "description": "localstorage persisted observables",
  "main": "dist/stored-observable.cjs.js",
  "module": "dist/stored-observable.es.js",
  "scripts": {
    "build": "BABEL_ENV=rollup rollup -c",
    "test": "BABEL_ENV=ava ava",
    "dev": "BABEL_ENV=ava ava -w",
    "pretest": "standard",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capaj/mobx-stored.git"
  },
  "keywords": [
    "mobx",
    "observable",
    "localstorage"
  ],
  "author": "capajj@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capaj/mobx-stored/issues"
  },
  "homepage": "https://github.com/capaj/mobx-stored#readme",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "localstorage-polyfill": "^1.0.1",
    "mobx": "^4.0.2",
    "prettier": "^1.9.2",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "standard": "^10.0.3",
    "babel-eslint": "^8.1.2",
    "eslint-plugin-prettier": "^2.4.0"
  },
  "peerDependencies": {
    "mobx": "^4.0.2"
  },
  "ava": {
    "files": [
      "**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "traverse": "^0.6.6"
  },
  "standard": {
    "ignore": [
      "stored-observable.js"
    ]
  }
}
